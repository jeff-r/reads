<% if @feeds.count == 0 %>
  <p>You have no feeds yet.</p>
<% else %>
  <div id='feed_title_bar'>
    Your feeds
    <%= link_to "New feed", new_feed_path, :class => 'titlecommand' %>
  </div>

  <table>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  
  <% @feeds.each do |feed| %>
    <tr>
      <td><%= link_to feed.title, feed %></td>
      <td><%= link_to '(Edit)', edit_feed_path(feed) %></td>
      <td><%= link_to '(Delete)', feed, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
  </table>
<% end %>

